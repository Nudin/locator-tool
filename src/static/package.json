{
  "name": "locator-tool",
  "version": "2.1.0",
  "description": "Add {{Location}} and {{Object location}} to images on Wikimedia Commons",
  "private": true,
  "main": "app/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dist": "webpack -p",
    "publish": "gh-pages -d dist -b gh-pages",
    "prettier": "prettier --print-width 100 --no-bracket-spacing --single-quote",
    "lint": "eslint app/ webpack.config.js && npm run prettier -- --list-different $(git ls-files '*.js')",
    "po": "angular-gettext-cli --compile --files 'po/*.po' --dest app/i18n.js --module app",
    "pot": "angular-gettext-cli --files '{,app/**/}*.{js,html}' --dest po/locator-tool.pot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simon04/locator-tool.git"
  },
  "author": "Simon Legner <Simon.Legner@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/simon04/locator-tool/issues"
  },
  "homepage": "https://tools.wmflabs.org/locator-tool/",
  "dependencies": {
    "angular": "1.5.11",
    "angular-gettext": "^2.3.8",
    "angular-ll-leaflet": "^0.2.2",
    "angular-local-storage": "^0.5.2",
    "angular-ui-router": "1.0.0-rc.1",
    "bootstrap": "^3.3.7",
    "core-js": "^2.4.1",
    "leaflet": "^1.0.3",
    "leaflet-geosearch": "1.0.0",
    "octicons": "^5.0.1"
  },
  "devDependencies": {
    "angular-gettext-cli": "^1.2.0",
    "angular-gettext-loader": "^1.0.1",
    "buble": "^0.15.2",
    "buble-loader": "^0.4.1",
    "clean-webpack-plugin": "^0.1.16",
    "compression-webpack-plugin": "^0.4.0",
    "css-loader": "^0.28.1",
    "eslint": "^3.19.0",
    "eslint-config-google": "^0.7.1",
    "eslint-config-prettier": "^2.1.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.1",
    "gh-pages": "^1.0.0",
    "html-loader": "^0.4.5",
    "html-webpack-plugin": "^2.28.0",
    "prettier": "^1.3.1",
    "style-loader": "^0.17.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1",
    "webpack-dev-server": "^2.4.5"
  }
}
