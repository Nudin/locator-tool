{
  "name": "locator-tool",
  "version": "2.1.0",
  "description": "Add {{Location}} to images on Wikimedia Commons",
  "main": "app/index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "dist": "webpack -p",
    "lint": "eslint app/",
    "po": "angular-gettext-cli --compile --files 'po/*.po' --dest app/i18n.js --module app",
    "pot": "angular-gettext-cli --files '{,app/**/}*.{js,html}' --dest po/locator-tool.pot"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/simon04/locator-tool.git"
  },
  "author": "Simon Legner <Simon.Legner@gmail.com>",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/simon04/locator-tool/issues"
  },
  "homepage": "https://tools.wmflabs.org/locator-tool/",
  "dependencies": {
    "angular": "1.5.9",
    "angular-gettext": "^2.3.8",
    "angular-ll-leaflet": "simon04/angular-ll-leaflet",
    "angular-local-storage": "^0.5.0",
    "angular-ui-router": "1.0.0-beta.3",
    "bootstrap": "^3.3.7",
    "leaflet": "^1.0.2",
    "leaflet-geosearch": "1.0.0"
  },
  "devDependencies": {
    "angular-gettext-cli": "^1.0.7",
    "angular-gettext-loader": "^1.0.1",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-runtime": "^6.20.0",
    "clean-webpack-plugin": "^0.1.14",
    "css-loader": "^0.26.1",
    "eslint": "^3.12.2",
    "eslint-config-google": "^0.6.0",
    "file-loader": "^0.9.0",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.25.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "2.2.0-rc.3",
    "webpack-dev-server": "2.2.0-rc.0"
  }
}
